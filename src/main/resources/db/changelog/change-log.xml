<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="script1" author="shmelev" runAlways="true" failOnError="false"  >
        <sqlFile
                dbms="postgresql"
                path="scripts/script1.sql"
                encoding="UTF-8"
                endDelimiter="/"
                relativeToChangelogFile="true"
                splitStatements="true"
                stripComments="true"/>
        <comment>Создание первой таблицы</comment>
    </changeSet>

    <changeSet id="script2" author="shmelev" runAlways="true" >
        <sqlFile
                dbms="postgresql"
                path="scripts/script2.sql"
                encoding="UTF-8"
                endDelimiter="/"
                relativeToChangelogFile="true"
                splitStatements="true"
                stripComments="true"/>
        <comment>Добавление записи с ФИО в таблицу student</comment>
    </changeSet>

</databaseChangeLog>
